{
    "name": "coenjacobs/migrator",
    "description": "Library to run database migrations in WordPress plugins",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Coen Jacobs",
            "email": "coenjacobs@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "CoenJacobs\\Migrator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CoenJacobs\\MigratorTests\\": "tests"
        }
    },
    "require": {
        "php": "^7.2|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.4"
    }
}
